Teams
{
team1 Endor_Imperials
team2 Endor_Rebels
}

mapgraphic "gfx/mplevels/siege_endor_village"

roundbegin_target "siege_start"
//this is a global target, it will be fired off if there is one when the round begins (this happens
//when there is at least 1 player on each team active)

//All message strings, like message_team1, wonround, etc. can be striped references by having the first char as @.

Endor_Imperials
{
RequiredObjectives 6 								//How many objectives must this team complete before winning the round?
UseTeam "Siege_Endor_Imperial" 							//Specifies the name of team "theme" to use
TeamIcon	"gfx/2d/mp_imp_symbol_3"
TeamColorOn	"1 0 0 1"
TeamColorOff	".7 0 0 1"	

Objective1
{
goalname "Shuttle_CP"
final 0 									//If 1, the round is won instantly for completeing this goal. If 0, counts toward total number of goals required. If -1, does not count toward the number of final goals.
message_team1 "We have recovered our stolen shuttle."		//Printed for team1 when this objective is completed.
message_team2 "The Imperials have taken the shuttle!"		//same as above, for team2.
//target shuttle_spawn_imp 					//Will use this target when this objective is completed. Remove this line for nothing.
sound_team1 "sound/endor/imp_shuttle_win.wav" 		//upon completion of objective, play this sound for team1. If you want no sound, remove this line.
sound_team2 "sound/endor/reb_shuttle_loose.wav" 		//same, for team2.
objdesc "The Stolen Imperial Shuttle Tyderium." 								//text description of objective
objgfx "gfx/mplevels/endor/shuttle_cp_imp" 			//graphic to display for objective
mapicon "gfx/mp/siegeicons/endor/shuttle"					//what graphic to display on the mapgraphic
litmapicon "gfx/mp/siegeicons/endor/shuttle_o"
donemapicon "gfx/mp/siegeicons/endor/shuttle_x"
mappos "152 268 16 16"							//what location to display
}

