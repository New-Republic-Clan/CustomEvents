gfx/effects/puffa1
{
	qer_editorimage	gfx/effects/puffa1
	surfaceparm	trans
	q3map_onlyvertexlighting
	cull	twosided
    {
        map gfx/effects/puffa1
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen vertex
    }
}

